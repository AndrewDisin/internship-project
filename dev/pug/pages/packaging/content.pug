+b.SECTION.content_packaging
	.container
		+b.UL.bread-crumbs
			+e.item
				+e.A.link(href="#") Головна
			+e.item
				+e.A.link_yellow-text Подарункова упаковка
		+b.goods-list
			each object in contentPackagin.goodsPackagin
				+b.ARTICLE.goods-item_packaging
					+e.A.link(href=object['link'])
						+b.FIGURE.item-img(data-text=object['dataText'])
							+e.IMG.img(src=object['srcImg'], alt=object['alt'])
						+e.title= object['title']