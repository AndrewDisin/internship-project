+b.SECTION.content_newyear
	.container
		+b.UL.bread-crumbs
			+e.item
				+e.A.link(href="#") Головна
			+e.item
				+e.A.link_red-text Новорічний декор
		+b.product-list
			each object in content.newYearProductList
				+b.ARTICLE.product-item
					+e.img
						+e.IMG.picture(src=object['srcImg'], alt= object['alt'])
					+e.title= object['title']
					+e.description= object['description']
					+e.price
						+e.cost= object['cost']
						+e.currency= object['currency']
						+e.serch
							+e.IMG.search-icon(src="../../../static/img/general/newyear/search.png", alt="Search")
						+e.like
							+e.IMG.like-icon(src="../../../static/img/general/newyear/like.png", alt="Like")
					+e.order= object['order']